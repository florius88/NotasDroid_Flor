<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.mi_expediente.MiExpedienteFragment">
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:src="@tools:sample/avatars" android:id="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="32dp" app:layout_constraintEnd_toStartOf="@+id/textViewExpedienteNombre"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.464"
            app:layout_constraintBottom_toTopOf="@+id/textViewExpedienteCiclo"
            app:layout_constraintVertical_bias="0.867"/>
    <TextView
            android:text="Apellidos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textViewExpedienteApellidos"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/textViewExpedienteEmail" android:layout_marginBottom="40dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="152dp"/>
    <TextView
            android:text="Nombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textViewExpedienteNombre"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/textViewExpedienteApellidos" android:layout_marginBottom="52dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="164dp"/>
    <TextView
            android:text="Ciclo"
            android:layout_width="146dp"
            android:layout_height="21dp" android:id="@+id/textViewExpedienteCiclo"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
            app:layout_constraintEnd_toStartOf="@+id/textViewExpedienteCurso"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintBottom_toTopOf="@+id/textViewExpedienteCalificacion"
            android:layout_marginBottom="44dp"/>
    <TextView
            android:text="Curso"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textViewExpedienteCurso"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="156dp"
            app:layout_constraintBottom_toTopOf="@+id/textViewExpedienteNotaMedia" android:layout_marginBottom="48dp"/>
    <TextView
            android:text="Calificacion Media"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textViewExpedienteCalificacion"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
            app:layout_constraintEnd_toStartOf="@+id/textViewExpedienteNotaMedia"
            app:layout_constraintHorizontal_bias="0.052"
            app:layout_constraintBottom_toTopOf="@+id/imageButtonExpedienteEmail" android:layout_marginBottom="100dp"/>
    <TextView
            android:text="Nota Media"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textViewExpedienteNotaMedia"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="132dp"
            app:layout_constraintBottom_toTopOf="@+id/imageButtonExpedienteFacebook"
            android:layout_marginBottom="92dp"/>
    <ImageButton
            android:src="@android:drawable/ic_dialog_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/imageButtonExpedienteEmail"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="44dp"
            app:layout_constraintEnd_toStartOf="@+id/imageButtonExpedienteTwitter"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="48dp"/>
    <ImageButton
            android:src="@android:drawable/btn_star_big_on"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/imageButtonExpedienteTwitter"
            app:layout_constraintEnd_toStartOf="@+id/imageButtonExpedienteFacebook" android:layout_marginEnd="44dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="52dp"/>
    <ImageButton
            android:src="@drawable/ic_launcher_foreground"
            android:layout_width="59dp"
            android:layout_height="54dp" android:id="@+id/imageButtonExpedienteFacebook"
            app:layout_constraintEnd_toStartOf="@+id/imageButtonExpedienteInstagram" android:layout_marginEnd="36dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="52dp"/>
    <ImageButton
            android:src="@drawable/ic_menu_camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/imageButtonExpedienteInstagram"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="60dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="52dp"/>
    <TextView
            android:text="Email"
            android:layout_width="166dp"
            android:layout_height="17dp" android:id="@+id/textViewExpedienteEmail"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="60dp"
            app:layout_constraintBottom_toTopOf="@+id/textViewExpedienteCurso" android:layout_marginBottom="32dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>